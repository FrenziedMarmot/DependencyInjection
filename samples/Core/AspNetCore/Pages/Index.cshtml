@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div>Interface says: @Model.InterfaceSvc.Greet()</div>
    <div>Impl says: @Model.ImplSvc.Greet()</div>
    <div>Factory-created says: @Model.FactoryInjectedSvc.Greet()</div>
    <div>Options says: <br/>- @Model.Options.Value.Value1<br/>- #@Model.Options.Value.Value3<br/>- @Model.Options.Value.Value5</div>
</div>